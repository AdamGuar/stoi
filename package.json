{
  "name": "stoi",
  "version": "1.0.0",
  "description": "",
  "main": "bin/index.js",
  "bin": "bin/index.js",
  "scripts": {
    "build": "tsc",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "start": "tsc && node ./bin/index.js --mode hide --text \"kot kot\" --imagePath \"./resources/cat.jpg\" --keyPath \"./keyOut/pkey.txt\" --secret \"totalysecretsecret\"",
    "start-hide": "tsc && node ./bin/index.js --mode hide --text \"kot kot\" --imagePath \"./resources/cat.jpg\" --imageOutPath \"./resources/encrypted.png\" --keyPath \"./keyOut/pkey.txt\" --secret \"totalysecretsecret\"",
    "start-find": "tsc && node ./bin/index.js --mode find --imagePath \"./resources/encrypted.png\" --keyPath \"./keyOut/pkey.txt\" --secret \"totalysecretsecret\" --textOut \"./resources/decrypted.txt\"",
    "start-keygen": "tsc && node ./bin/index.js --mode keygen --secret \"totalysecretsecret\" --boundary 250x250 --publicKeyOut \"./keyOut/pkey.txt\""
  },
  "author": "Adam Wozniak",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "chai": "^4.3.6",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "canvas": "^2.10.1",
    "command-line-args": "^5.2.1",
    "cryptr": "^6.0.3",
    "randomstring": "^1.2.2"
  }
}
